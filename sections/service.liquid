<div class="container service">
  <h2 class="heading text-center"><span>{{ section.settings.m_title }}</span></h2>    
    <div class="row deliveryinfo rless">
        {% if section.settings.s_title %}
            <div class="col-12 col-sm-4 col-md-4 cless">
                <ul>
                    <li>
                      <span>
                        {% render 'icon-allservice',icon: section.settings.icon1 %}
                        {% if section.settings.checkiconimage1 %}
                          {% if section.settings.iconimage1 != blank %}
                          <img loading="lazy" src="{{ section.settings.iconimage1 | img_url: 'master' }}" alt="{{ section.settings.iconimage1.alt }}" class="img-fluid mx-auto" width="{{ section.settings.iconimage1.width }}" height="{{ section.settings.iconimage1.height }}"/>
                          {% endif %}
                        {% endif %}
                      </span>
                  </li>
                    <li class="text-left">
                        <h2>{{ section.settings.header_gift }}</h2>
                        <p>{{ section.settings.header_gift1 }}</p>
                    </li>
                </ul>
            </div>
        {% endif %}
        {% if section.settings.sa_title %}
            <div class="col-12 col-sm-4 col-md-4 cless">
                <ul>
                    <li>
                      <span>
                        {% render 'icon-allservice', icon: section.settings.icon2 %}
                        {% if section.settings.checkiconimage2 %}
                          {% if section.settings.iconimage2 != blank %}
                          <img loading="lazy" src="{{ section.settings.iconimage2 | img_url: 'master' }}" alt="{{ section.settings.iconimage2.alt }}" class="img-fluid mx-auto" width="{{ section.settings.iconimage2.width }}" height="{{ section.settings.iconimage2.height }}"/>
                          {% endif %}
                        {% endif %}
                      </span>
                  </li>
                    <li class="text-left">
                        <h2>{{ section.settings.header_pay }}</h2>
                        <p>{{ section.settings.header_pay1 }}</p>
                    </li>
                </ul>
            </div>
        {% endif %}
        {% if section.settings.sb_title %}
            <div class="col-12 col-sm-4 col-md-4 cless">
                <ul>
                    <li>
                      <span>
                        {% render 'icon-allservice', icon: section.settings.icon3 %}
                        {% if section.settings.checkiconimage3 %}
                          {% if section.settings.iconimage3 != blank %}
                          <img loading="lazy" src="{{ section.settings.iconimage3 | img_url: 'master' }}" alt="{{ section.settings.iconimage3.alt }}" class="img-fluid mx-auto" width="{{ section.settings.iconimage3.width }}" height="{{ section.settings.iconimage3.height }}"/>
                          {% endif %}
                        {% endif %}
                      </span>
                  </li>
                    <li class="text-left">
                        <h2>{{ section.settings.header_support }}</h2>
                        <p>{{ section.settings.header_support1 }}</p>
                    </li>
                </ul>
            </div>
        {% endif %}
    </div>
</div>
  
